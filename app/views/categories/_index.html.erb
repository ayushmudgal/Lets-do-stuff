<div class="header"><%= 'Categories' %></div>
<table>
  <tbody>
    <% @categories.each do |category| %>
      <tr>
        <td>
        <%= link_to (category.name), category %>
        </td>
        <!-- <td><%= link_to 'Destroy', category, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

